{
  "name": "supercharge",
  "description": "Supercharge application boilerplate",
  "version": "0.0.0",
  "dependencies": {
    "@supercharge/console": "~3.7.1",
    "@supercharge/contracts": "~3.7.1",
    "@supercharge/core": "~3.7.1",
    "@supercharge/encryption": "~3.7.1",
    "@supercharge/facades": "~3.7.1",
    "@supercharge/hashing": "~3.7.1",
    "@supercharge/http": "~3.7.1",
    "@supercharge/session": "~3.7.1",
    "@supercharge/strings": "~1.28.0",
    "@supercharge/view": "~3.7.1",
    "@supercharge/vite": "~3.7.1"
  },
  "devDependencies": {
    "@supercharge/eslint-config-typescript": "~2.3.0",
    "@supercharge/tsconfig": "~4.0.0",
    "@swc-node/register": "~1.5.1",
    "@types/node": "~18.7.13",
    "eslint": "~8.22.0",
    "nodemon": "~2.0.19",
    "typescript": "~4.7.4",
    "vite": "~3.0.9"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:js": "vite build",
    "lint": "eslint . --ext .js,.ts",
    "dev": "npm run dev:server",
    "dev:js": "vite",
    "dev:server": "nodemon --exec npm start --ext ts,hbs --ignore test --ignore public",
    "start": "node -r @swc-node/register server.ts",
    "test": "echo 'Missing tests.' && exit 1"
  }
}
